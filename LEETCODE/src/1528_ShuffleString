class Solution {
    public String restoreString(String s, int[] indices) {
        int l=s.length();
        char ch[]=new char[l];
        for(int i=0;i<l;i++)
        {
            char c=s.charAt(i);
            int index=indices[i];
            ch[index]=c;
        }
        return String.valueOf(ch);
    }
}
